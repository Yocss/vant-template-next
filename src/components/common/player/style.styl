@import '~@/assets/css/common/variables.styl'
.axmine-player
  width 100%
  height 100%
  user-select none
  &.hide-controll-bar .vjs-has-started
    .vjs-control-bar
      display: none
  &.hide-controll-bar .axmine-player-box
    &.vjs-playing.vjs-user-active
      .axmine-player-controll
        display: none
  &-box
    font-size 12PX
    user-select none
    .axmine-player-controll
      position absolute
      left 0
      top 0
      width 100%
      height 100%
      padding-bottom 3em
      background-size: 4em 4em
      background-position: center center
      background-repeat: no-repeat
    &.vjs-playing.vjs-user-active
      .axmine-player-controll
        opacity .5
        background-color transtarent
        background-image url('data:image/png;base64,')
    &.vjs-paused.vjs-ended
      .axmine-player-controll
        background none
        // background-image url('data:image/png;base64,')
    &.vjs-paused
      .axmine-player-controll
        opacity .8
        background-color rgba(#000, .3)
        background-image url('data:image/png;base64,')
    .vjs-progress-holder
      height .1em
      // background-color #0f0
    // .vjs-slider
    //   background-color #fff
    .vjs-play-progress:before
      top 50%
      margin-top: -.45em
    .vjs-picture-in-picture-control
      display: none
    // 显示视频剩余时间
    &.video-js.vjs-layout-small .vjs-remaining-time
      display: block !important
      span
        display: none
      .vjs-remaining-time-display
        display: initial
        user-select none
    .vjs-poster
      background-size: 100% 100%
    .vjs-big-play-button
      display: none
      width 2.5em
      border-radius 1.25em
    &.vjs-big-play-centered
      .vjs-big-play-button
        margin-left -1.25em