.dot
  overflow hidden
  word-wrap normal
  white-space nowrap
  text-overflow ellipsis
.dot2
.dot--l2
.dot--l3
.dot3
  display: -webkit-box
  overflow: hidden
  text-overflow: ellipsis
  -webkit-line-clamp: 2
  -webkit-box-orient: vertical
.dot3
.dot--l3
  -webkit-line-clamp: 3

// 背景毛玻璃效果
.filter
  backdrop-filter blur(15px)
  -webkit-backdrop-filter blur(15px)
// 页面转场效果
@import './transition.styl'
